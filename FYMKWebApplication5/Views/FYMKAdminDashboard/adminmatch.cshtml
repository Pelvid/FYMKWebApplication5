                                : (e.val("").prop("disabled", !0),
                                    l.find(".no-result").remove(),
                                    l.find("tr").show()),
                                $("#rowcount").html($(".filterable tr").length - 1);
                        });

                    var filterTable = (el) => {
                        console.log(el.target, el);
                        var e = $(el.target),
                            l = e.val().toLowerCase(),
                            n = e.parents(".filterable"),
                            i = n.find(".filters th").index(e.parents("th")),
                            r = n.find(".table"),
                            o = r.find("tbody tr");
                        if (l) {
                            let d = o.filter(function () {
                                return (
                                    -1 ===
                                    $(el.target)
                                        .find("td")
                                        .eq(i)
                                        .text()
