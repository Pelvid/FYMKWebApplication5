                                        <div class="modal-dialog">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <h5 class="modal-title" id="exampleModalLabel">Matching Process</h5>
                                                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                                </div>
                                                <div class="modal-body">
                                                    <table class="table">
                                                        <thead class="table-dark">
                                                            <tr>
                                                                <th scope="col">FirstName</th>
                                                                <th scope="col">Enterpreneurship</th>
                                                                <th scope="col">Employment</th>
                                                                <th scope="col">University</th>
                                                                <th scope="col">Building</th>
                                                                <th scope="col">Developing</th>
                                                                <th scope="col">Resilence</th>
                                                                <th scope="col">Volunteering</th>
                                                                <th scope="col">College</th>
                                                                <th scope="col">Career</th>
                                                                <th scope="col">CareerOptions</th>
                                                                <th scope="col">CV</th>
                                                                <th scope="col">Id</th>
                                                                <th scope="col">Match Status</th>
                                                                <th scope="col">Submit</th>

                                                            </tr>

                                                        </thead>

                                                        <tbody class="table-dark">
                                                            @{

                                                                foreach (var Dat in adminMentees)
                                                                {


                                                                    //string MatchID = "Match"
                                                                    <tr>
                                                                        <td>@Dat.FirstName</td>
                                                                        <td>@Dat.Enterpreneurship</td>
                                                                        <td>@Dat.Employment</td>
                                                                        <td>@Dat.University</td>
                                                                        <td>@Dat.Building</td>
                                                                        <td>@Dat.Developing</td>
                                                                        <td>@Dat.Resilence</td>
                                                                        <td>@Dat.Volunteering</td>
                                                                        <td>@Dat.College</td>
                                                                        <td>@Dat.Career</td>
                                                                        <td>@Dat.CareerOptions</td>
                                                                        <td>@Dat.CV</td>
                                                                        <td>@Dat.Id</td>

                                                                        <td>

                                                                            <div>

                                                                                <input type="checkbox" name="matching" id="@Dat.Id">Matched<br>


                                                                                <button onclick="check('@Dat.Id')">"Matched"</button>
                                                                                <button onclick="uncheck('@Dat.Id')">"Un-Matched"</button>

                                                                            </div>
                                                                            @*@foreach (var Datt in Check)
                                                                             {  
                                                                               <div>

                                                                                     <input type="checkbox" name="matching" id="@Datt.Id">Matched<br>


                                                                                    <button onclick="check('@Datt.Id')">"Matched"</button>
                                                                                    <button onclick="uncheck('@Datt.Id')">"Un-Matched"</button>

                                                                                </div> 
                                                                        
                                                                             }*@

                                                                        </td>

                                                                        

                                                                    </tr>

                                                                   

                                                                }



                                                            }
