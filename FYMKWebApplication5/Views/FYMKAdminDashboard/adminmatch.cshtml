                                    o = r.find("tbody tr:visible");
                                if (o.length === 0) {
                                    o.show()
                                }
                                console.log(l);

                                let d = o.filter(function () {
                                    return (
                                        -1 ===
                                        $(this)
                                            .find("td")
                                            .eq(i)
                                            .text()
                                            .toLowerCase()
                                            .indexOf(l)

                                    );
                                });

                                r.find("tbody .no-result").remove(),
                                    o.show(),
